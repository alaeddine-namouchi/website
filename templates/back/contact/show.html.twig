{% extends 'back/base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contact.name }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contact.message }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ contact.object }}</td>
            </tr>
            <tr>
                <th>EmailAdress</th>
                <td>{{ contact.emailAdress }}</td>
            </tr>
            <tr>
                <th>MobileNumber</th>
                <td>{{ contact.mobileNumber }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contact_index') }}">back to list</a>

    <a href="{{ path('app_contact_edit', {'id': contact.id}) }}">edit</a>

    {{ include('back/contact/_delete_form.html.twig') }}
{% endblock %}
