{% extends 'back/base.html.twig' %}

{% block title %} Response{% endblock %}

{% block body %}
    <h1>Response</h1>

    <table class="table">
        <tbody>
            {# <tr>
                <th>Id</th>
                <td>{{ response.id }}</td>
            </tr> #}
            <tr>
                <th>Object</th>
                <td>{{ response.object }}</td>
            </tr>
            <tr>
                <th>Response</th>
                <td>{{ response.response }}</td>
            </tr>
            <tr>
                <th>Submited</th>
                <td>{{ response.submited ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_response_index') }}">back to list</a>

    <a href="{{ path('app_response_edit', {'id': response.id}) }}">edit</a>

    {{ include('back/response/_delete_form.html.twig') }}
{% endblock %}
