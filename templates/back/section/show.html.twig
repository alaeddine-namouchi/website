{% extends 'back/base.html.twig' %}

{% block title %}Section{% endblock %}

{% block body %}
    <h1>Section</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ section.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ section.label }}</td>
            </tr>
            <tr>
                <th>Alias</th>
                <td>{{ section.alias }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ section.description }}</td>
            </tr>
            <tr>
                <th>BusinessName</th>
                <td>{{ section.businessName }}</td>
            </tr>
            <tr>
                <th>AllAction</th>
                <td>{{ section.allAction ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_section_index') }}">back to list</a>

    <a href="{{ path('app_section_edit', {'id': section.id}) }}">edit</a>

    {{ include('back/section/_delete_form.html.twig') }}
{% endblock %}
