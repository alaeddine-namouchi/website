{% extends 'back/base.html.twig' %}

{% block title %}Profile{% endblock %}

{% block body %}
    <h1>Profile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ profile.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ profile.label }}</td>
            </tr>
            <tr>
                <th>Alias</th>
                <td>{{ profile.alias }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ profile.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_profile_index') }}">back to list</a>

    <a href="{{ path('app_profile_edit', {'id': profile.id}) }}">edit</a>

    {{ include('back/profile/_delete_form.html.twig') }}
{% endblock %}
