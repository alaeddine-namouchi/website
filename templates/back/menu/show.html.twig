{% extends 'back/base.html.twig' %}

{% block title %}Menu{% endblock %}

{% block body %}
    <h1>Menu</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ menu.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ menu.label }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ menu.link }}</td>
            </tr>
            <tr>
                <th>Emplacement</th>
                <td>{{ menu.emplacement }}</td>
            </tr>
            <tr>
                <th>TypeMenu</th>
                <td>{{ menu.typeMenu }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_menu_index') }}">back to list</a>

    <a href="{{ path('app_menu_edit', {'id': menu.id}) }}">edit</a>

    {{ include('back/menu/_delete_form.html.twig') }}
{% endblock %}
