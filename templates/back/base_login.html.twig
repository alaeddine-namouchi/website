		<link rel="stylesheet" href="{{ asset('css/theme.css') }}" />
		<!-- Theme Custom CSS -->
		<link rel="stylesheet" href="{{ asset('css/custom.css') }}">
	    <!-- Head Libs -->
		<script src="{{ asset('vendor/modernizr/modernizr.js') }}"></script>
		<script src="{{ asset('master/style-switcher/style.switcher.localstorage.js') }}"></script>

	</head>
	<body>
		<!-- start: page -->
		<section class="body-sign">
			<div class="center-sign">
            {# float-left #}
				<a href="/" class="logo " style="margin-left: 30%!important">
					<img src="/img/transtu_logo.png" height="70" alt="Porto Admin" />
				</a>

				<div class="panel card-sign" >
					<div class="card-title-sign mt-3 text-end" style=" color: #ffc107!important;  ">
						<h2 class="title text-uppercase font-weight-bold m-0" style="    background-color: #1266ad!important;font-size:16px; color:#ffc107;    padding: 0px 12px;">
						<i class="bx bx-user-circle me-1 text-6 position-relative top-5"></i> Sign In</h2>
					</div>
					<div class="card-body" style="border-top-color: #ffc107!important;">
						<form  method="post">
                         {% if error %}
                            <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                        {% endif %}

                        {% if app.user %}
                            <div class="mb-3">
                                You are logged in as {{ app.user.username }}, <a href="{{ path('app_logout') }}">Logout</a>
                            </div>
                        {% endif %}
							<div class="form-group mb-3">
								<label>Username</label>
								<div class="input-group">
									<input type="email" value="{{ last_username }}" name="email" class="form-control form-control-lg" autocomplete="email" required autofocus />
									<span class="input-group-text" style="    color: #ed9c28;">
										<i class="bx bx-user text-4"></i>
									</span>
								</div>
							</div>

							<div class="form-group mb-3">
								<div class="clearfix">
									<label class="float-left">Password</label>
									<a href="pages-recover-password.html" class="float-end" style="    color: #1266ad;font-weight:bold">Lost Password?</a>
								</div>
								<div class="input-group">
									<input  type="password" name="password" id="inputPassword"  class="form-control form-control-lg" />
									<span class="input-group-text" style="    color: #ed9c28;">
										<i class="bx bx-lock text-4"></i>
									</span>
								</div>
							</div>
                              <input type="hidden" name="_csrf_token"   value="{{ csrf_token('authenticate') }}" />
							<div class="row">
								<div class="col-sm-8">
									<div class="checkbox-custom checkbox-default">
										<input id="RememberMe" name="rememberme" type="checkbox"/>
										<label for="RememberMe">Remember Me</label>
									</div>
								</div>
								<div class="col-sm-4 text-end">
									<button type="submit" class="btn btn-primary mt-2" style="    background-color: #198754!important;font-size:16px; color:#ffc107; font-weight:bold;   padding: 5px 12px;">Sign In</button>
								</div>
							</div>

							{# <span class="mt-3 mb-3 line-thru text-center text-uppercase">
								<span>or</span>
							</span> #}

							

							{# <p class="text-center">Don't have an account yet? <a href="pages-signup.html">Sign Up!</a></p> #}

						</form>
					</div>
				</div>

				<p class="text-center text-muted mt-3 mb-3">&copy; Copyright 2021. All Rights Reserved.</p>
			</div>
		</section>
		<!-- end: page -->
		<!-- Vendor -->

		<script src="{{ asset('vendor/jquery/jquery.js')}}"></script>
		<script src="{{ asset('vendor/jquery-browser-mobile/jquery.browser.mobile.js')}}"></script>
		<script src="{{ asset('vendor/popper/umd/popper.min.js')}}"></script>
		<script src="{{ asset('vendor/bootstrap/js/bootstrap.bundle.min.js')}}"></script>
		<script src="{{ asset('vendor/bootstrap-datepicker/js/bootstrap-datepicker.js')}}"></script>
		<script src="{{ asset('vendor/common/common.js')}}"></script>
		<script src="{{ asset('vendor/nanoscroller/nanoscroller.js')}}"></script>
		<script src="{{ asset('vendor/magnific-popup/jquery.magnific-popup.js')}}"></script>
		<script src="{{ asset('vendor/jquery-placeholder/jquery.placeholder.js')}}"></script>
